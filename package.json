{
  "name": "bluegate",
  "version": "1.0.0",
  "description": "Promise based web framework",
  "main": "bluegate.js",
  "scripts": {
    "test": "mocha -b test.js && eslint *.js",
    "coverage": "mocha --require ./node_modules/blanket-node/bin/index.js -R html-cov test.js > coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mauritsl/bluegate.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "config": {
    "blanket": {
      "pattern": "bluegate.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "keywords": [
    "promises",
    "web",
    "framework",
    "rest",
    "http",
    "connect"
  ],
  "author": "Maurits Lawende",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.9.25",
    "body-parser": "^1.12.3",
    "compression": "^1.4.3",
    "connect": "^3.3.5",
    "cookie-parser": "^1.3.4"
  },
  "devDependencies": {
    "blanket-node": "^2.0.0",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "eslint": "^0.20.0",
    "mocha": "^2.2.4",
    "needle": "^0.9.2"
  }
}
