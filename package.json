{
  "name": "bluegate",
  "version": "1.1.0",
  "description": "Promise based web framework",
  "main": "bluegate.js",
  "scripts": {
    "test": "(multi='spec=- mocha-lcov-reporter=lcov.txt' mocha -b --require ./blanket.js -t 5s -R mocha-multi test.js) && eslint *.js",
    "coverage": "mocha --require ./blanket.js -R html-cov test.js > coverage.html",
    "rapidity": "mocha --require rapidity -R rapidity test.js > performance.html"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mauritsl/bluegate.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "config": {
    "blanket": {
      "pattern": "bluegate.js",
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "keywords": [
    "promises",
    "web",
    "framework",
    "rest",
    "http",
    "connect"
  ],
  "author": "Maurits Lawende",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^2.9.25",
    "body-parser": "^1.12.3",
    "compression": "^1.4.3",
    "connect": "^3.3.5",
    "cookie-parser": "^1.3.4",
    "forwarded-for": "^1.0.0",
    "lodash": "^3.8.0",
    "retrieve-arguments": "^1.0.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^2.3.0",
    "chai-as-promised": "^5.0.0",
    "eslint": "^0.21.0",
    "mocha": "^2.2.4",
    "mocha-lcov-reporter": "0.0.2",
    "mocha-multi": "^0.7.1",
    "needle": "^0.9.2",
    "rapidity": "^1.0.1"
  }
}
